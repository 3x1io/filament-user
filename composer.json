{
    "name": "3x1io/filament-user",
    "description": "User Manager Resource For Filament Admin",
    "keywords": [
        "laravel",
        "user",
        "cli",
        "resource",
        "ui",
        "filament"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\FilamentUser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "bezhansalleh/filament-shield": "^2.1",
        "filament/filament": "^2.9",
        "spatie/laravel-package-tools": "^1.12",
        "stechstudio/filament-impersonate": "^2.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "io3x1\\FilamentUser\\FilamentUserProvider"
            ]
        }
    },
    "prefer-stable": true
}
